egate = delegate","        \/\/ Создание объекта карты","        mapView = MapView(configuration: configuration, delegate: delegate)","    }","}","","\/\/ Экран с картой","struct ContentView: View {","    @StateObject private var viewModel = ContentViewModel()","","    var body: some View {","        Map(view: viewModel.mapView)","            .frame(maxWidth: .infinity, maxHeight: .infinity)","    }","}"],"syntax":"swift","type":"codeListing"}],"kind":"content"}],"variants":[{"paths":["\/documentation\/mapsnativesdk\/startusing"],"traits":[{"interfaceLanguage":"swift"}]}],"abstract":[{"type":"text","text":"Интегрируйте карты в свое приложение на основе UIKit или SwiftUI."}],"identifier":{"url":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/StartUsing","interfaceLanguage":"swift"},"references":{"doc://MapsNativeSDK/documentation/MapsNativeSDK/MapView":{"navigatorTitle":[{"kind":"identifier","text":"MapView"}],"role":"symbol","abstract":[{"text":"Main object of the Map","type":"text"}],"identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/MapView","title":"MapView","url":"\/documentation\/mapsnativesdk\/mapview","fragments":[{"kind":"keyword","text":"class"},{"text":" ","kind":"text"},{"kind":"identifier","text":"MapView"}],"kind":"symbol","type":"topic"},"addedFrameworks":{"alt":"Added frameworks","variants":[{"url":"\/images\/MapsNativeSDK\/addedFrameworks.png","traits":["1x","light"]}],"type":"image","identifier":"addedFrameworks"},"doc://MapsNativeSDK/documentation/MapsNativeSDK/MapStyle":{"kind":"symbol","navigatorTitle":[{"text":"MapStyle","kind":"identifier"}],"title":"MapStyle","abstract":[],"fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"MapStyle","kind":"identifier"}],"role":"symbol","url":"\/documentation\/mapsnativesdk\/mapstyle","type":"topic","identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/MapStyle"},"doc://MapsNativeSDK/documentation/MapsNativeSDK/MapConfiguration":{"abstract":[{