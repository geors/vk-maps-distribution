ide func viewDidLoad() {","        super.viewDidLoad()","        \/\/ Конфигурирование SDK","        MapsSDKConfigurator.setup(apiKey: \"...\")","        \/\/ Создание конфигурации карты","        let configuration = MapConfiguration(center: Coordinates(lng: 37.6156, lat: 55.7522), zoom: 10, predefinedStyle: .main)","        \/\/ Создание объекта карты","        let mapView = MapView(frame: CGRect(x: 0, y: 0,","                                            width: UIScreen.main.bounds.width,","                                            height: UIScreen.main.bounds.height),","                              configuration: configuration,","                              delegate: self)","        \/\/ Размещение карты на экране","        view.addSubview(mapView)","    }","}","","\/\/ Делегат карты","extension ViewController: MapViewDelegate {}"],"type":"codeListing","syntax":"swift"},{"level":4,"text":"Использование в SwiftUI-проекте","anchor":"%D0%98%D1%81%D0%BF%D0%BE%D0%BB%D1%8C%D0%B7%D0%BE%D0%B2%D0%B0%D0%BD%D0%B8%D0%B5-%D0%B2-SwiftUI-%D0%BF%D1%80%D0%BE%D0%B5%D0%BA%D1%82%D0%B5","type":"heading"},{"type":"paragraph","inlineContent":[{"text":"Для использования карты в SwiftUI необходимо создать объект ","type":"text"},{"type":"codeVoice","code":"MapView"},{"type":"text","text":" и передать его в структуру "},{"type":"codeVoice","code":"Map"},{"text":".","type":"text"}]},{"syntax":"swift","code":["\/\/ Делегат для MapView","class BaseMapViewDelegate: MapViewDelegate {}","","\/\/ View-модель экрана, где будет использоваться карта","final class ContentViewModel: ObservableObject {","    \/\/ Объект карты","    let mapView: MapView","    \/\/ Объект делегата карты","    private let mapViewDelegate: MapViewDelegate","    ","    init() {","        \/\/ Конфигурирование SDK","        MapsSDKConfigurator.setup(apiKey: \"...\")","        \/\/ Создание конфигурации карты","        let configuration = MapConfiguration(center: Coordinates(lng: 37.6156, lat: 55.7522), zoom: 10, predefinedStyle: .main)","        \/\/ Создание делегата карты","        let delegate = BaseMapViewDelegate()","        mapViewDelegate = delegate","        \/\/ Создание объекта карты","        mapView = MapView(configuration: configuration, delegate: delegate)","    }","}","","\/\/ Экран с картой","struct ContentView: View {","    @StateObject private var viewModel = ContentViewModel()","","    var body: some View {","        Map(view: viewModel.mapView)","            .frame(maxWidth: .infinity, maxHeight: .infinity)","    }","}"],"type":"codeListing"}]}],"variants":[{"paths":["\/documentation\/mapsnativesdk\/startusing"],"traits":[{"interfaceLanguage":"swift"}]}],"metadata":{"role":"article","title":"Начало работы с MapsNativeSDK","modules":[{"name":"MapsNativeSDK"}],"roleHeading":"Article"},"references":{"doc://MapsNativeSDK/documentation/MapsNativeSDK/MapView":{"navigatorTitle":[{"text":"MapView","kind":"identifier"}],"fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"MapView"}],"type":"topic","title":"MapView","identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/MapView","kind":"symbol","abstract":[{"type":"text","text":"Main object of the Map"}],"role":"symbol","url":"\/documentation\/mapsnativesdk\/mapview"},"doc://MapsNativeSDK/documentation/MapsNativeSDK":{"identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK","title":"MapsNativeSDK","role":"collection","type":"topic","kind":"symbol","url":"\/documentation\/mapsnativesdk","abstract":[{"type":"strong","inlineContent":[{"type":"text","text":"MapsNativeSDK"}]},{"type":"text","text":" - библиотека, позволяющая использовать картографические данные и геосервисы VK в ваших iOS-приложениях."}]},"https://dev.vk.com/ru/vkmaps/general-information/api-key":{"titleInlineContent":[{"text":"портале","type":"text"}],"type":"link","title":"портале","identifier":"https:\/\/dev.vk.com\/ru\/vkmaps\/general-information\/api-key","url":"https:\/\/dev.vk.com\/