egate: MapViewDelegate {}","","\/\/ ...","","\/\/ Создание делегата","let delegate = BaseMapViewDelegate()"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"type":"text","text":"В дальнейшем данный объект потребуется передать при создании экземпляра типа "},{"identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/MapView","isActive":true,"type":"reference"},{"type":"text","text":"."}],"type":"paragraph"},{"name":"Note","type":"aside","style":"note","content":[{"inlineContent":[{"type":"text","text":"Не требуется реализовывать методы "},{"isActive":true,"type":"reference","identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/MapViewDelegate"},{"type":"text","text":", если их функциональность вам не нужна."}],"type":"paragraph"}]},{"text":"Размещение карт на экране","level":3,"anchor":"%D0%A0%D0%B0%D0%B7%D0%BC%D0%B5%D1%89%D0%B5%D0%BD%D0%B8%D0%B5-%D0%BA%D0%B0%D1%80%D1%82-%D0%BD%D0%B0-%D1%8D%D0%BA%D1%80%D0%B0%D0%BD%D0%B5","type":"heading"},{"type":"paragraph","inlineContent":[{"text":"MapsNativeSDK могут быть использованы в приложениях, основанных на UIKit и SwiftUI. Далее рассмотрим каждую из платформ.","type":"text"}]},{"name":"Warning","type":"aside","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Убедитесь, что к этому моменту вы сконфигурировали SDK с помощью вызова "},{"type":"codeVoice","code":"MapsSDKConfigurator.setup"},{"text":".","type":"text"}]}],"style":"warning"},{"text":"Использование в UIKit-проекте","level":4,"anchor":"%D0%98%D1%81%D0%BF%D0%BE%D0%BB%D1%8C%D0%B7%D0%BE%D0%B2%D0%B0%D0%BD%D0%B8%D0%B5-%D0%B2-UIKit-%D0%BF%D1%80%D0%BE%D0%B5%D0%BA%D1%82%D0%B5","type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Для использования карты в UIKit необходимо создать объект "},{"type":"reference","identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/MapView","isActive":true},{"text":", передав в него конфигурацию, и разместить созданный объект на экране.","type":"text"}]},{"type":"codeListing","syntax":"swift","code":["class ViewController: UIViewController {","","    override func viewDidLoad() {","        super.viewDidLoad()","        \/\/ Конфигурирование SDK","        MapsSDKConfigurator.setup(apiKey: \"...\")","        \/\/ Создание конфигурации карты","        let configuration = MapConfiguration(center: Coordinates(lng: 37.6156, lat: 55.7522), zoom: 10, predefinedStyle: .main)","        \/\/ Создание объекта карты","        let mapView = MapView(frame: CGRect(x: 0, y: 0,","                                            width: UIScreen.main.bounds.width,","                                            height: UIScreen.main.bounds.height),","                              configuration: configuration,","                              delegate: self)","        \/\/ Размещение карты на экране","        view.addSubview(mapView)","    }","}","","\/\/ Делегат карты","extension ViewController: MapViewDelegate {}"]},{"type":"heading","anchor":"%D0%98%D1%81%D0%BF%D0%BE%D0%BB%D1%8C%D0%B7%D0%BE%D0%B2%D0%B0%D0%BD%D0%B8%D0%B5-%D0%B2-SwiftUI-%D0%BF%D1%80%D0%BE%D0%B5%D0%BA%D1%82%D0%B5","text":"Использование в SwiftUI-проекте","level":4},{"inlineContent":[{"type":"text","text":"Для использования карты в SwiftUI необходимо создать объект "},{"type":"codeVoice","code":"MapView"},{"text":" и передать его в структуру ","type":"text"},{"type":"codeVoice","code":"Map"},{"type":"text","text":"."}],"type":"paragraph"},{"code":["\/\/ Делегат для MapView","class BaseMapViewDelegate: MapViewDelegate {}","","\/\/ View-модель экрана, где будет использоваться карта","final class ContentViewModel: ObservableObject {","    \/\/ Объект карты","    let mapView: MapView","    \/\/ Объект делегата карты","    private let mapViewDelegate: MapViewDelegate","    ","    init() {","        \/\/ Конфигурирование SDK","        MapsSDKConfigurator.setup(apiKey: \"...\")","        \/\/ Создание конфигурации карты","        let configuration = MapConfiguration(center: Coordinates(lng: 37.6156, lat: 55.7522), zoom: 10, predefinedStyle: .main)","        \/\/ Создание делегата карты","        let delegate = BaseMapViewDelegate()","        mapViewDelegate = delegate","        \/\/ Создание объекта карты","        mapView = MapView(configuration: configuration, delegate: delegate)","    }","}","","\/\/ Экран с картой","struct ContentView: View {","    @StateObject private var viewModel = ContentViewModel()","","    var body: some View {","        Map(view: viewModel.mapView)","            .frame(maxWidth: .infinity, maxHeight: .infinity)","    }","}"],"syntax":"swift","type":"codeListing"}],"kind":"content"}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/mapsnativesdk\/startusing"]}],"schemaVersion":{"patch":0,"minor":3,"major":0},"references":{"doc://MapsNativeSDK/documentation/MapsNativeSDK/Styles":{"kind":"article","type":"topic","url":"\/documentation\/mapsnativesdk\/styles","title":"Оформление карты","identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/Styles","role":"article","abstract":[{"text":"Изменяйте внешний вид карты, рисуйте геометрические фигуры и размещайте маркеры.","type":"text"}]},"doc://MapsNativeSDK/documentation/MapsNativeSDK":{"kind":"symbol","identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK","type":"topic","role":"collection","title":"MapsNativeSDK","url":"\/documentation\/mapsnativesdk","abstract":[{"inlineContent":[{"text":"MapsNativeSDK","type":"text"}],"type":"strong"},{"type":"text","text":" - библиотека, позволяющая использовать картографические данные и геосервисы VK в ваших iOS-приложениях."}]},"doc://MapsNativeSDK/documentation/MapsNativeSDK/MapConfiguration":{"abstract":[{"text":"Configuration of Map","type":"text"}],"type":"topic","role":"symbol","url":"\/documentation\/mapsnativesdk\/mapconfiguration","kind":"symbol","identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/MapConfiguration","title":"MapConfiguration","fragments":[{"tex