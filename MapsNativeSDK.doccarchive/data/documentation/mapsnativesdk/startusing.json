зданный объект на экране."}],"type":"paragraph"},{"type":"codeListing","code":["class ViewController: UIViewController {","","    override func viewDidLoad() {","        super.viewDidLoad()","        \/\/ Конфигурирование SDK","        MapsSDKConfigurator.setup(apiKey: \"...\")","        \/\/ Создание конфигурации карты","        let configuration = MapConfiguration(center: Coordinates(lng: 37.6156, lat: 55.7522), zoom: 10, predefinedStyle: .main)","        \/\/ Создание объекта карты","        let mapView = MapView(frame: CGRect(x: 0, y: 0,","                                            width: UIScreen.main.bounds.width,","                                            height: UIScreen.main.bounds.height),","                              configuration: configuration,","                              delegate: self)","        \/\/ Размещение карты на экране","        view.addSubview(mapView)","    }","}","","\/\/ Делегат карты","extension ViewController: MapViewDelegate {}"],"syntax":"swift"},{"anchor":"%D0%98%D1%81%D0%BF%D0%BE%D0%BB%D1%8C%D0%B7%D0%BE%D0%B2%D0%B0%D0%BD%D0%B8%D0%B5-%D0%B2-SwiftUI-%D0%BF%D1%80%D0%BE%D0%B5%D0%BA%D1%82%D0%B5","type":"heading","level":4,"text":"Использование в SwiftUI-проекте"},{"inlineContent":[{"type":"text","text":"Для использования карты в SwiftUI необходимо создать объект "},{"code":"MapView","type":"codeVoice"},{"type":"text","text":" и передать его в структуру "},{"type":"codeVoice","code":"Map"},{"type":"text","text":"."}],"type":"paragraph"},{"code":["\/\/ Делегат для MapView","class BaseMapViewDelegate: MapViewDelegate {}","","\/\/ View-модель экрана, где будет использоваться карта","final class ContentViewModel: ObservableObject {","    \/\/ Объект карты","    let mapView: MapView","    \/\/ Объект делегата карты","    private let mapViewDelegate: MapViewDelegate","    ","    init() {","        \/\/ Конфигурирование SDK","        MapsSDKConfigurator.setup(apiKey: \"...\")","        \/\/ Создание конфигурации карты","        let configuration = MapConfiguration(center: Coordinates(lng: 37.6156, lat: 55.7522), zoom: 10, predefinedStyle: .main)","        \/\/ Создание делегата карты","        let delegate = BaseMapViewDelegate()","        mapViewDelegate = delegate","        \/\/ Создание объекта карты","        mapView = MapView(configuration: configuration, delegate: delegate)","    }","}","","\/\/ Экран с картой","struct ContentView: View {","    @StateObject private var viewModel = ContentViewModel()","","    var body: some View {","        Map(view: viewModel.mapView)","            .frame(maxWidth: .infinity, maxHeight: .infinity)","    }","}"],"type":"codeListing","syntax":"swift"}],"kind":"content"}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/mapsnativesdk\/startusing"]}],"references":{"doc://MapsNativeSDK/documentation/MapsNativeSDK/MapConfiguration":{"type":"topic","role":"symbol","title":"MapConfiguration","abstract":[{"text":"Configuration of Map","type":"text"}],"identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/MapConfiguration","navigatorTitle":[{"text":"MapConfiguration","kind":"identifier"}],"kind":"symbol","url":"\/documentation\/mapsnativesdk\/mapconfiguration","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"MapConfiguration"}]},"doc://MapsNativeSDK/documentation/MapsNativeSDK/MapView":{"navigatorTitle":[{"text":"MapView","kind":"identifier"}],"kind":"symbol","title":"MapView","abstract":[{"text":"Main object of the Map","type":"text"}],"fragments":[{"kind":"keyword","text":"class"},{"text":" ","kind":"text"},{"text":"MapView","kind":"identifier"}],"url":"\/documentation\/mapsnativesdk\/mapview","type":"topic","identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/MapView","role":"symbol"},"doc://MapsNativeSDK/documentation/MapsNativeSDK/MapViewDelegate":{"kind":"symbol","navigatorTitle":[{"text":"MapViewDelegate","kind":"identifier"}],"role":"symbol","title