{","        switch event {","            \/\/ ...","            \/\/ event.type = .cameraDidMove","            \/\/ ...","        }","    }","}"]},{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"Пример отслеживания событий загрузки карты"}]}]},{"syntax":"swift","code":["\/\/ БЫЛО","class MapDelegate: MapViewDelegate{","    func mapViewDidLoad(_: MapView) {","        \/\/ ...","    }","}","","\/\/ СТАЛО","class MapDelegate: MapViewDelegate{","    func mapView(_ map: MapView, didReceiveEvent event: MapEvent) {","        switch event {","            \/\/ ...","            \/\/ event.type = .mapDidShow","            \/\/ ...","        }","    }","}"],"type":"codeListing"},{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"Пример отслеживания событий тапа по карте"}]}]},{"type":"codeListing","syntax":"swift","code":["\/\/ БЫЛО","class MapDelegate: MapViewDelegate{","    func mapView(_: MapView, didTapCoords coords: Coordinates, uiCoords: CGPoint) {}","    func mapView(_: MapView, didLongTapCoords coords: Coordinates, uiCoords: CGPoint) {}","}","","\/\/ СТАЛО","class MapDelegate: MapViewDelegate{","    func mapView(_ map: MapView, didReceiveEvent event: MapEvent) {","        switch event {","            \/\/ ...","            \/\/ event.type = .tap","            \/\/ или","            \/\/ event.type = .longTap","            \/\/ ...","        }","    }","}"]},{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"Пример отслеживания событий смены режима следования"}]}]},{"type":"codeListing","code":["\/\/ БЫЛО","class MapDelegate: MapViewDelegate{","    func mapView(_: MapView, didChangeMode mode: MapMode) {}","}","","\/\/ СТАЛО","class MapDelegate: MapViewDelegate{","    func mapView(_: MapView, willChangeModeTo nextMode: MapMode) -> MapMode {}","    func mapView(_: MapView, didChangeModeTo mode: MapMode) {}","}"],"syntax":"swift"},{"inlineCont