eiveEvent event: MapEvent) {","        switch event {","            \/\/ ...","            \/\/ event.type = .cameraDidMove","            \/\/ ...","        }","    }","}"],"syntax":"swift"},{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"text":"Пример отслеживания событий загрузки карты","type":"text"}]}]},{"type":"codeListing","code":["\/\/ БЫЛО","class MapDelegate: MapViewDelegate{","    func mapViewDidLoad(_: MapView) {","        \/\/ ...","    }","}","","\/\/ СТАЛО","class MapDelegate: MapViewDelegate{","    func mapView(_ map: MapView, didReceiveEvent event: MapEvent) {","        switch event {","            \/\/ ...","            \/\/ event.type = .mapDidShow","            \/\/ ...","        }","    }","}"],"syntax":"swift"},{"inlineContent":[{"type":"strong","inlineContent":[{"text":"Пример отслеживания событий тапа по карте","type":"text"}]}],"type":"paragraph"},{"code":["\/\/ БЫЛО","class MapDelegate: MapViewDelegate{","    func mapView(_: MapView, didTapCoords coords: Coordinates, uiCoords: CGPoint) {}","    func mapView(_: MapView, didLongTapCoords coords: Coordinates, uiCoords: CGPoint) {}","}","","\/\/ СТАЛО","class MapDelegate: MapViewDelegate{","    func mapView(_ map: MapView, didReceiveEvent event: MapEvent) {","        switch event {","            \/\/ ...","            \/\/ event.type = .tap","            \/\/ или","            \/\/ event.type = .longTap","            \/\/ ...","        }","    }","}"],"syntax":"swift","type":"codeListing"},{"type":"paragraph","inlineContent":[{"inlineContent":[{"text":"Пример отслеживания событий смены режима следования","type":"text"}],"type":"strong"}]},{"syntax":"swift","code":["\/\/ БЫЛО","class MapDelegate: MapViewDelegate{","    func mapView(_: MapView, didChangeMode mode: MapMode) {}","}","","\/\/ СТАЛО","class MapDelegate: MapViewDelegate{","    func mapView(_: MapView, willChangeModeTo nextMode: MapMode) -> MapMode {}","    func mapView(_: MapView, didChangeModeTo mode: MapMode)